x = 2 ;

y = ((3 * 4) - x) ;

z = (x + (4 * y)) ;

if(z < 10)
{
    while(z >= 0)
    {
        for(i = 0 ; i < 100 ; i = (i + 1))
        {
            z = (z * 2) ;
        }
        z = (z - 1) ;
    }
}
else
{
    if(0 < (x - (2 * z)))
    {
        max = x ;
    }
    else
    {
        max = (z - 2) ;
    }
}

